{
  "name": "exprml/exprml-php",
  "description": "The ExprML Interpreter in PHP",
  "license": "BSD-2-Clause",
  "require": {
    "google/protobuf": "^4.28",
    "symfony/yaml": "^5.4.44",
    "ext-mbstring": "*",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Exprml\\": [
        "src/Exprml/",
        "build/gen/pb/Exprml/"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "platform": {
      "php": "7.4"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6.21"
  }
}
