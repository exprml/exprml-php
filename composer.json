{
  "name": "exprml/exprml-php",
  "description": "The ExprML Interpreter in PHP",
  "license": "BSD-2-Clause",
  "require": {
    "google/protobuf": "^4.28",
    "symfony/yaml": "^7.1",
    "mustangostang/spyc": "^0.6.3",
    "justinrainbow/json-schema": "^6.0",
    "ext-mbstring": "*"
  },
  "autoload": {
    "psr-4": {
      "Exprml\\": [
        "src/Exprml/",
        "build/gen/pb/Exprml/"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "platform": {
      "php": "8.3.11"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^11.3"
  }
}
